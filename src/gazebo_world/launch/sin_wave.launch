<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="world" default="$(find gazebo_world)/worlds/sin_wave.world"/>
  <arg name="paused" default="false"/>
  <arg name="use_sim_time" default="true"/>
  <arg name="gui" default="true"/>
  <arg name="headless" default="false"/>
  <arg name="debug" default="false"/>

  <param name="robot_description" command="$(find xacro)/xacro --inorder $(find gazebo_world)/urdf/sin_wave.urdf.xacro"/>

  <node name="gazebo" pkg="gazebo" type="gazebo" respawn="false" output="screen" args="$(arg world)">
    <env name="GAZEBO_MODEL_PATH" value="$(find gazebo_world)/models:${GAZEBO_MODEL_PATH}" />
    <env name="GAZEBO_PLUGIN_PATH" value="$(find gazebo_world)/plugins/build:${GAZEBO_PLUGIN_PATH}" />
  </node>
</launch>
